{
    "pipelines": {
        "default": [
            {
                "step": {
                    "name": "Running my custom pipe",
                    "script": [
                        {
                            "pipe": "docker://<DockerAccountName>/<ImageName>:<version>",
                            "variables": {
                                "USERNAME": "$My_username",
                                "PASSWORD": "$Password"
                            }
                        }
                    ]
                }
            }
        ]
    }
}